install-blitzHEADERS: $(blitz_HEADERS)
	@$(NORMAL_INSTALL)
	test -z "$(blitzdir)" || $(mkdir_p) "$(DESTDIR)$(blitzdir)"
	@list='$(blitz_HEADERS)'; for p in $$list; do \
	  if test -f "$$p"; then d=; else d="$(srcdir)/"; fi; \
	  f=$(am__strip_dir) \
	  echo " $(blitzHEADERS_INSTALL) '$$d$$p' '$(DESTDIR)$(blitzdir)/$$f'"; \
	  $(blitzHEADERS_INSTALL) "$$d$$p" "$(DESTDIR)$(blitzdir)/$$f"; \
	done

