set terminal lua fulldoc \
solid originreset plotsize 9,6.5
set output 'plotExecTimePyCppDelay.tex'

set format x "%d"
set format y "%g"

unset key

set xlabel 'Monte Carlo cycles'  
set xrange [10000 : 110000] noreverse nowriteback
set mxtics 5

set ylabel 'Relative execution time, (timeC++ - timePy)/timeC++'
set yrange [0 : 2.5] noreverse nowriteback
set mytics 5


plot 'execTimeHeCpp.data' using 1:2 lt 2 lw 3 w lp

