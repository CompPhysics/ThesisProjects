#include <TestCpp.h>

// class definition
TestCpp::TestCpp(int N_, const MyArray<double>& A): N(N_){
	someArray.redim(N, N) = A;	// Now someArray = A and someArray.size = NxN
}


// print someArray
void TestCpp::print_(){

	cout << "Printing from C++" << endl;

	for(int i=0; i<N; i++){
		for(int j=0; j<N; j++){
			cout << someArray(i,j) << endl;	
		}
	}
}
